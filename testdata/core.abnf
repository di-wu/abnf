ALPHA  = %x41-5A / %x61-7A ; A-Z / a-z
BIT    = "0" / "1"
CHAR   = %x01-7F
CR     =  %x0D ; carriage return
CRLF   = CR LF
CTL    = %x00-1F / %x7F ; controls
DIGIT  = %x30-39 ; 0-9
DQUOTE = %x22
HEXDIG =  DIGIT / "A" / "B" / "C" / "D" / "E" / "F"
HTAB   = %x09
LF     = %x0A
LWSP   = *(WSP / CRLF WSP)
OCTET  = %x00-FF
SP     = %x20
VCHAR  = %x21-7E
WSP    = SP / HTAB
